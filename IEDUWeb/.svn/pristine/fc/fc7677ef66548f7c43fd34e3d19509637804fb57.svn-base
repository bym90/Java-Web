 <body>
  <script>
		function Show(){
			var age1="24";
			var age2=24;
			var result=document.getElementById("test");
			if(age1 === age2){
				result.innerHTML="두 데이터는 같다";
			}
			else{
				result.innerHTML="두 데이터는 다르다";
			}
		}
	</script>

	<p id="test">출력위치</p>
	<input type="button" value="Click" onClick="Show()">
 </body>
